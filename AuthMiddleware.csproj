<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>ViniVerse.AuthMiddleware</PackageId>
    <Version>1.0.1</Version>
    <Authors>Vinicius Martins</Authors>
    <Company>Vini-Verse</Company>
    <Description>JWT Authentication middleware for ASP.NET Core</Description>
    <PackageProjectUrl>https://github.com/Vini-Verse/AuthMiddleware</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Vini-Verse/AuthMiddleware</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>jwt;authentication;aspnetcore;dotnet</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Authentication" Version="2.3.0" />
  </ItemGroup>

</Project>
